<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yidong.mapper.UserMapper" >

  <select id="selectByAccount" resultType="com.yidong.model.User" parameterType="java.lang.String" >
    select
     account,password,id,name
    from yd_user
    where account = #{account}
  </select>

  <select id="getUser"  resultType="com.yidong.model.User">
    select * from yd_user where id=#{id}

  </select>
</mapper>